{
  "rewrites": [
    {
      "source": "/",
      "destination": "https://1digitalpartner.github.io/tanzi-blog/"
    },
    {
      "source": "/(.*)",
      "destination": "https://1digitalpartner.github.io/tanzi-blog/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=3600"
        },
        {
          "key": "X-Proxy-From",
          "value": "tanzitech-vercel"
        }
      ]
    }
  ]
}
